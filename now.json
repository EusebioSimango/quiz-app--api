{
  "version": 2,
  "builds": [
    { 
      "src": "/./src/server.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/./src/server.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/.*",
      "dest": "/./src/server.js",
      "methods": [
        "POST"
      ]
    }
  ]
}
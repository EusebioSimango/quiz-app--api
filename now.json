{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/./src/server.js",
      "methods": [
        "GET"
      ]
    },
    {
      "src": "/.*",
      "dest": "/./src/server.js",
      "methods": [
        "POST"
      ]
    }
  ]
}